{
  "name": "skippy",
  "version": "0.0.4",
  "description": "Simple and robust dependency container.",
  "keywords": [
    "container",
    "dependency",
    "dependency injection",
    "di",
    "injection",
    "service"
  ],
  "main": "index.js",
  "scripts": {
    "bdd": "mocha --colors --reporter=dot --watch --recursive test",
    "coverage": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- --reporter=dot --recursive test",
    "coverage-upload": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint --color .",
    "test": "mocha --colors --reporter=dot --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rezzza/skippy.git"
  },
  "author": "Armand Abric <armand@forgebinaire.net>",
  "license": "MIT",
  "devDependencies": {
    "chai": "~3.2.0",
    "coveralls": "~2.11.3",
    "eslint": "~0.24.1",
    "istanbul": "~0.3.17",
    "mocha": "~2.2.5",
    "sinon": "~1.15.4"
  },
  "dependencies": {
    "immutable": "~3.7.4"
  }
}
